
MODULE IfElseIf;

VAR
    x, y : INTEGER;

    PROCEDURE func(a:INTEGER) : INTEGER;
        BEGIN
            IF (a=4) THEN
                RETURN 4
            ELSIF (a=3) THEN 
                RETURN 4    
            ELSIF (a=2) THEN
                RETURN 2
            ELSIF (a=1) THEN
                RETURN 1
            ELSE
                RETURN 0
        END

    END func

END IfElseIf.